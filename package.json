{
  "name": "jackgracie-sveltekit",
  "version": "1.0.0",
  "private": true,
  "description": "Jack Gracie Portfolio - SvelteKit SPA",
  "author": "jcgithu",
  "keywords": [
    "svelte",
    "sveltekit",
    "portfolio",
    "spa"
  ],
  "scripts": {
    "dev": "bun run vite dev",
    "build": "bun run vite build",
    "preview": "bun run vite preview",
    "check": "bun run svelte-kit sync && bun run svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun run svelte-kit sync && bun run svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.46.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/animejs": "^3.1.13",
    "mdsvex": "^0.12.6",
    "sass": "^1.93.2",
    "svelte": "^5.39.11",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "dependencies": {
    "@sveltejs/enhanced-img": "^0.8.4",
    "animejs": "^4.2.2",
    "lottie-web": "^5.13.0",
    "poline": "^0.11.0",
    "runed": "^0.34.0",
    "zod": "^4.1.12"
  },
  "type": "module"
}
